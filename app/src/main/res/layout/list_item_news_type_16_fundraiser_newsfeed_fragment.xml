            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


         <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/list_item_news_type_16_newsfeed_fragment__icon_holder_background_shadow_item_info_icon"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="8dp"
            android:background="@drawable/main_activity_info_fragment_icon_imageview_circle_shadow_background"
            android:elevation="13dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent">

             <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/list_item_news_type_16_newsfeed_fragment__icon_info_holder_background_white_item_icon"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/main_activity_info_fragment_icon_imageview_circle_white_background"
                android:elevation="3dp">

                <com.fishpott.fishpott5.Views.CircleImageView
                    android:id="@+id/list_item_news_type_16_newsfeed_fragment__icon_info_imageView_item_icon"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:contentDescription="@string/none"
                    android:elevation="4dp"
                    android:padding="3dp"
                    android:src="@drawable/info_icon_hollow"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

             </androidx.constraintlayout.widget.ConstraintLayout>

         </androidx.constraintlayout.widget.ConstraintLayout>

         <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/list_item_news_type_16_newsfeed_fragment_icon_holder_background_shadow_more_images_icon_holder"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp"
            android:background="@drawable/rectangle_horizontal_orientation_rounded_corners_shadow_background"
            android:elevation="13dp"
            app:layout_constraintBottom_toBottomOf="@+id/list_item_news_type_16_newsfeed_fragment_news_image_imageview"
            app:layout_constraintEnd_toEndOf="@+id/list_item_news_type_16_newsfeed_fragment_news_image_imageview">

            <TextView
                android:id="@+id/list_item_news_type_16_newsfeed_fragment_more_images_textview"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:elevation="4dp"
                android:text="@string/sample_more_images_number_10"
                android:textAlignment="center"
                android:textColor="@color/colorPrimary"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
         </androidx.constraintlayout.widget.ConstraintLayout>

     </androidx.constraintlayout.widget.ConstraintLayout>

