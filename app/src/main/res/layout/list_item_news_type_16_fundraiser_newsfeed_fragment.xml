        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="5dp"
        android:text="@string/test_pottname_testpottnameraylight"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="@dimen/list_item_news_type_1_newsfeed_fragment_pottname_text_size"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/list_item_news_type_16_newsfeed_fragment_pottpic_relativeLayout"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/list_item_news_type_16_newsfeed_fragment_fullname_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="@string/testfullname_anno_dankyi"
        android:textColor="@color/colorUpdateActivityBackground"
        android:textSize="@dimen/list_item_news_type_1_newsfeed_fragment_pottname_text_size"
        app:layout_constraintStart_toEndOf="@+id/list_item_news_type_16_newsfeed_fragment_pottpic_relativeLayout"
        app:layout_constraintTop_toBottomOf="@+id/list_item_news_type_16_newsfeed_fragment_pottname_textview" />


    <ImageView
