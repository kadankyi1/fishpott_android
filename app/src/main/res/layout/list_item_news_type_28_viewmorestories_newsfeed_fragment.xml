<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/colorAccent">


    <RelativeLayout
        android:id="@+id/list_item_news_type_28_newsfeed_fragment_relativelayout_1"
        android:layout_width="@dimen/list_item_news_type_28_newsfeed_fragment_relativelayout_1_size"
        android:layout_height="@dimen/list_item_news_type_28_newsfeed_fragment_relativelayout_1_size"
        android:layout_margin="10dp"
        android:elevation="2dp"
        android:background="@drawable/list_item_news_type_28_justnews_circle_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RelativeLayout
            android:id="@+id/list_item_news_type_28_newsfeed_fragment_relativelayout_2"
            android:layout_width="@dimen/list_item_news_type_28_newsfeed_fragment_relativelayout_1_size"
            android:layout_height="@dimen/list_item_news_type_28_newsfeed_fragment_relativelayout_1_size"
            android:layout_margin="3dp"
            android:layout_centerInParent="true"
            android:elevation="3dp"
            android:background="@drawable/main_activity_info_fragment_icon_imageview_circle_white_background"
            >

            <android.support.constraint.ConstraintLayout
                android:id="@+id/list_item_news_type_28_newsfeed_fragment_relativelayout_3"
                android:layout_width="@dimen/list_item_news_type_28_newsfeed_fragment_relativelayout_1_size"
                android:layout_height="@dimen/list_item_news_type_28_newsfeed_fragment_relativelayout_1_size"
                android:layout_margin="4dp"
                android:layout_centerInParent="true"
                android:elevation="4dp"
                android:background="@drawable/main_activity_info_fragment_icon_imageview_circle_white_background"
                >

                <TextView
                    android:id="@+id/list_item_news_type_28_newsfeed_fragment_view_more_textview"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginStart="16dp"
                    android:elevation="5dp"
                    android:gravity="center"
                    android:text="@string/list_item_news_type_28_newsfeed_fragment_view_more"
                    android:textAlignment="center"
                    android:textColor="@color/colorPrimaryDark"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </android.support.constraint.ConstraintLayout>

        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="@dimen/list_item_news_type_28_newsfeed_fragment_pottpic_holder_relativelayout_1_size"
        android:layout_height="@dimen/list_item_news_type_28_newsfeed_fragment_pottpic_holder_relativelayout_1_size"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:elevation="5dp"
        android:visibility="gone"
        android:background="@drawable/main_activity_info_fragment_icon_imageview_circle_shadow_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/list_item_news_type_28_newsfeed_fragment_relativelayout_1">

        <com.fishpott.fishpott5.Views.CircleImageView
            android:id="@+id/list_item_news_type_28_newsfeed_fragment_pott_pic_circleimageview"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:contentDescription="@string/none"
            android:elevation="6dp"
            app:srcCompat="@drawable/list_item_suggestedlinkupsactivity_activity_businessicon_imageview" />

    </RelativeLayout>


    <android.support.constraint.ConstraintLayout
        android:layout_width="@dimen/list_item_news_type_28_newsfeed_fragment_pottpic_holder_relativelayout_2_size"
        android:layout_height="@dimen/list_item_news_type_28_newsfeed_fragment_pottpic_holder_relativelayout_2_size"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/main_activity_info_fragment_icon_imageview_circle_shadow_background"
        android:elevation="5dp"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <android.support.constraint.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/list_item_news_type_28_justnews_circle_background"
            android:elevation="6dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/news_reposted_additem_price_textview"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:elevation="7dp"
                android:text="@string/list_item_news_type_28_newsfeed_fragment_sample_price_5555"
                android:textColor="@color/colorAccent"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </android.support.constraint.ConstraintLayout>

    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>
